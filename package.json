{
  "name": "cross-chain-liquidity-aggregator",
  "version": "1.0.0",
  "description": "A comprehensive DeFi protocol for cross-chain liquidity aggregation built on Stacks blockchain",
  "main": "index.js",
  "scripts": {
    "test": "clarinet test",
    "check": "clarinet check",
    "console": "clarinet console",
    "integrate": "clarinet integrate",
    "deploy:testnet": "clarinet deploy --network testnet",
    "deploy:mainnet": "clarinet deploy --network mainnet",
    "coverage": "clarinet test --coverage",
    "lint": "eslint scripts/ --ext .js,.ts",
    "format": "prettier --write scripts/ docs/",
    "build": "npm run check && npm run test",
    "dev": "clarinet integrate --watch",
    "docs:serve": "docsify serve docs",
    "docs:build": "docsify build docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/7zak/cross-chain-liquidity-aggregator.git"
  },
  "keywords": [
    "defi",
    "cross-chain",
    "liquidity",
    "aggregator",
    "stacks",
    "bitcoin",
    "clarity",
    "smart-contracts",
    "amm",
    "dex"
  ],
  "author": "Abdulrazak malik <hexchange001@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/7zak/cross-chain-liquidity-aggregator/issues"
  },
  "homepage": "https://github.com/7zak/cross-chain-liquidity-aggregator#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0",
    "docsify-cli": "^4.4.4"
  },
  "dependencies": {
    "@stacks/transactions": "^6.5.0",
    "@stacks/network": "^6.5.0",
