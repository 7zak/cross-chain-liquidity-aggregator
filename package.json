    "@stacks/wallet-sdk": "^6.5.0",
    "@stacks/blockchain-api-client": "^7.0.0",
    "@stacks/connect-react": "^20.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "clarinet": {
    "version": "1.8.0",
    "requirements": []
  },
  "funding": {
    "type": "github",
